{
  "name": "wah-api",
  "version": "3.214.0",
  "license": "MIT",
  "main": "serverless.ts",
  "engines": {
    "node": ">=14.15.0"
  },
  "scripts": {
    "start": "sls offline start",
    "build": "sls package",
    "package": "sls package",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "deploy-dev-eu": "sls deploy -r eu-west-1 -s dev",
    "deploy-dev-us": "sls deploy -r us-west-1 -s dev",
    "deploy-dev-kr": "sls deploy -r ap-northeast-2 -s dev",
    "deploy-dev": "npm run deploy-dev-us && npm run deploy-dev-eu && npm run deploy-dev-kr",
    "deploy-eu": "sls deploy -r eu-west-1 -s prod",
    "deploy-se": "sls deploy -r eu-north-1 -s prod",
    "deploy-us": "sls deploy -r us-west-1 -s prod",
    "deploy-kr": "sls deploy -r ap-northeast-2 -s prod",
    "deploy": "npm run deploy-eu && npm run deploy-us && npm run deploy-kr",
    "update-json-data-locally": "cd ../scripts;sh update-data.sh; cd .."
  },
  "private": true,
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "@ukon1990/js-utilities": "^1.9.3",
    "@ukon1990/subscription-manager": "^1.0.2",
    "aws-jwt-verify": "^3.1.0",
    "aws-sdk": "^2.516.0",
    "file-saver": "^2.0.2",
    "jwt-decode": "^3.0.0-beta.2",
    "mysql": "^2.17.1",
    "njwt": "^1.0.0",
    "promise-throttle": "^1.0.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "secure-random": "^1.1.2",
    "source-map-explorer": "^2.1.0",
    "sqlstring": "^2.3.2",
    "tslib": "^1.9.3",
    "uuidv4": "^6.2.5",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/sqlstring": "^2.2.1",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "requirejs": "^2.3.6",
    "serverless-esbuild": "^1.32.6",
    "source-map-support": "^0.5.21",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.0.4",
    "tslint": "~5.18.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "esbuild": "^0.14.11",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-offline": "^8.7.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
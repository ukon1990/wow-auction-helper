{
  "name": "wah-api",
  "version": "3.122.0",
  "license": "MIT",
  "main": "handler.js",
  "scripts": {
    "start": "sls offline start --useSeparateProcesses",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "deploy-dev-eu": "sls deploy -r eu-west-1 -s dev",
    "deploy-dev-us": "sls deploy -r us-west-1 -s dev",
    "deploy-dev-kr": "sls deploy -r ap-northeast-2 -s dev",
    "deploy-dev": "npm run deploy-dev-us && npm run deploy-dev-eu && npm run deploy-dev-kr",
    "deploy-eu": "sls deploy -r eu-west-1 -s prod",
    "deploy-us": "sls deploy -r us-west-1 -s prod",
    "deploy-kr": "sls deploy -r ap-northeast-2 -s prod",
    "deploy": "npm run deploy-us && npm run deploy-eu && npm run deploy-kr",
    "update-json-data-locally": "cd ../scripts;sh update-data.sh; cd .."
  },
  "private": true,
  "dependencies": {
    "@ukon1990/js-utilities": "^1.9.3",
    "@ukon1990/subscription-manager": "^1.0.2",
    "aws-sdk": "^2.516.0",
    "file-saver": "^2.0.2",
    "mysql": "^2.17.1",
    "promise-throttle": "^1.0.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "source-map-explorer": "^2.1.0",
    "sqlstring": "^2.3.2",
    "tslib": "^1.9.3",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.31",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.6.8",
    "@types/sqlstring": "^2.2.1",
    "@types/supertest": "^2.0.8",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "requirejs": "^2.3.6",
    "serverless": "^1.50.0",
    "serverless-offline": "^5.10.1",
    "serverless-webpack": "^5.3.1",
    "source-map-support": "^0.5.13",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.0.4",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "3.5.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.2"
  }
}

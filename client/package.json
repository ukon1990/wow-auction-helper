  "main": "handler.js",
  "scripts": {
    "ng": "ng",
    "ng-serve-secondary": "ng serve --port 4201",
    "start": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve",
    "build": "ng build --configuration production",
    "test-build": "ng lint && npm test:headless --single-run && ng e2e && ng build --configuration production",
    "test": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng test",
    "test:coverage": "ng test --code-coverage",
    "test:headless": "ng test --browsers ChromeHeadless --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze": "source-map-explorer 'dist/*.js'",
    "ng-http-serve-prod": "ng build --configuration production && http-server -p 8080 -c-1 dist",
    "update-json-data-locally": "cd ../scripts;sh update-data.sh; cd ..",
    "clean-install": "rm -rf package-lock.json; rm -rf node_modules; npm i;"
  },
  "private": true,
